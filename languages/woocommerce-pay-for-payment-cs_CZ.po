#: admin/class-pay4pay-admin.php:44 woocommerce-payforpayment.php:70
#, php-format
msgid ""
"WooCommerce Pay4Payment requires at least WooCommerce %s. Please update!"
msgstr ""
"WooCommerce Pay4Payment vyžaduje alespoň WooCommerce %s. Prosím aktualizujte!"

#: admin/class-pay4pay-admin.php:62 admin/class-pay4pay-admin.php:283
#: woocommerce-payforpayment.php:32
msgid "Extra Charge"
msgstr "Poplatek za platbu"

#: admin/class-pay4pay-admin.php:68
msgid "Item Title"
msgstr "Název poplatku"

#: admin/class-pay4pay-admin.php:70
msgid "This will show up in the shopping basket."
msgstr "Zobrazí se v nákupním košíku."

#: admin/class-pay4pay-admin.php:74
msgid "Fixed charge"
msgstr "Pevný poplatek"

#: admin/class-pay4pay-admin.php:76
msgid "Extra charge to be added to cart when this payment method is selected."
msgstr ""
"Poplatek navíc, který bude přidán do košíku pokud bude vybrána tato platební "
"metoda."

#: admin/class-pay4pay-admin.php:83
msgid "Percent charge"
msgstr "Procentuální poplatek"

#: admin/class-pay4pay-admin.php:85
msgid "Percentage of cart total to be added to payment."
msgstr "Procento z celkové ceny košíku, které se přidá k platbě."

#: admin/class-pay4pay-admin.php:96
msgid "Charge at least"
msgstr "Nejméně"

#: admin/class-pay4pay-admin.php:98
msgid ""
"Minimum extra charge to be added to cart when this payment method is "
"selected."
msgstr ""
"Minimální poplatek, který bude přidán do košíku, když bude vybrána tato "
"platební metoda."

#: admin/class-pay4pay-admin.php:106
msgid "Charge at most"
msgstr "Maximálně"

#: admin/class-pay4pay-admin.php:108
msgid ""
"Maximum extra charge to be added to cart when this payment method is "
"selected. Enter zero to disable."
msgstr ""
"Maximální  poplatek, který bude přidán do košíku, když bude vybrána tato "
"platební metoda."

#: admin/class-pay4pay-admin.php:116
msgid "Disable on Free Shipping"
msgstr "Zakázat při dopravě zdarma"

#: admin/class-pay4pay-admin.php:117
msgid "Don’t charge this fee when free shipping is available."
msgstr "Neuplatňovat poplatek pokud je k dispozici platba zdarma."

#: admin/class-pay4pay-admin.php:128
msgid "Extra Charge Taxes"
msgstr "Poplatek za platbu DPH"

#: admin/class-pay4pay-admin.php:133
msgid "Taxable"
msgstr "zdanitelný"

#: admin/class-pay4pay-admin.php:135
msgid "Payment fee is taxable"
msgstr "Poplatek je zdanitelný"

#: admin/class-pay4pay-admin.php:142
msgid "Inclusive Taxes"
msgstr "bez DPH"

#: admin/class-pay4pay-admin.php:144
msgid "The payment fee is inclusive of taxes."
msgstr "Poplatek zahrnuje i DPH."

#: admin/class-pay4pay-admin.php:145
msgid ""
"If you leave this unchecked taxes will be calculated on top of the payment "
"fee."
msgstr ""
"Ponecháte-li toto pole prázdné, DPH bude připočítáno k poplatku za platbu."

#: admin/class-pay4pay-admin.php:151
msgid "Tax class"
msgstr "Daňová třída"

#: admin/class-pay4pay-admin.php:153
msgid "Select a the tax class applied to the extra charge."
msgstr "Vyberte daňovou třídu, která bude použita na poplatek."

#: admin/class-pay4pay-admin.php:166
msgid "Include in percental payment fee calculation:"
msgstr "Zahrnout do kalkulace procentuálního poplatku:"

#: admin/class-pay4pay-admin.php:172
msgid "Fees"
msgstr "Poplatky"

#: admin/class-pay4pay-admin.php:174
msgid "Include fees in calculation."
msgstr "Zahrnout poplatky při výpočtu."

#: admin/class-pay4pay-admin.php:180
msgid "Coupons"
msgstr "Kupóny"

#: admin/class-pay4pay-admin.php:182
msgid "Include Coupons in calculation."
msgstr "Zahrnout kupóny při výpočtu."

#: admin/class-pay4pay-admin.php:188
msgid "Shipping"
msgstr "Doprava"

#: admin/class-pay4pay-admin.php:190
msgid "Include shipping cost in calculation."
msgstr "Zahrnout cenu dopravy při výpočtu."

#: admin/class-pay4pay-admin.php:199
msgid "Taxes"
msgstr "DPH"

#: admin/class-pay4pay-admin.php:201
msgid "Include taxes in calculation."
msgstr "Zahrnout DPH při výpočtech."

#: admin/class-pay4pay-admin.php:295
#, php-format
msgctxt "Gateway list column"
msgid "%s %% of cart totals"
msgstr "%s %% z ceny objednávky"

#: woocommerce-payforpayment.php:249
msgid "Standard"
msgstr "Standardní"
